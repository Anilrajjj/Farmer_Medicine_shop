{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/about.html",
      "dest": "/about.html"
    },
    {
      "src": "/cart.html", 
      "dest": "/cart.html"
    },
    {
      "src": "/login.html",
      "dest": "/login.html"
    },
    {
      "src": "/signup.html",
      "dest": "/signup.html"
    },
    {
      "src": "/checkout.html",
      "dest": "/checkout.html"
    },
    {
      "src": "/order-confirmation.html",
      "dest": "/order-confirmation.html"
    },
    {
      "src": "/forget.html",
      "dest": "/forget.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
